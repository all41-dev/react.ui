{
  "name": "@all41-dev/react.ui",
  "type": "module",
  "description": "React UI component library with DataGrid, Tooltip, and Toast components",
  "main": "./dist/react-ui.cjs",
  "module": "./dist/react-ui.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/all41-dev/react.ui.git#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-ui.js",
      "require": "./dist/react-ui.cjs"
    },
    "./styles": "./dist/react.ui.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "tsc -p tsconfig.build.json && vite build --mode lib",
    "lint": "eslint .",
    "preview": "vite preview",
    "preversion": "npm run build:lib --prod",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.19",
    "lucide-react": "^0.545.0",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.64.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-tooltip": "^5.29.1",
    "sonner": "^2.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
